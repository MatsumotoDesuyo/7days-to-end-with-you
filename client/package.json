    "!/src/*.test.ts",
    "!/src/*.test.tsx",
    "!/src/**/__snapshots__",
    "/public",
    "/dist"
  ],
  "scripts": {
    "start": "webpack serve --config webpack.dev.js",
    "lint": "eslint . --ext .tsx --ext .ts --fix",
    "clean": "rimraf dist/*",
    "prebuild": "npm run clean",
    "build": "webpack --config webpack.prod.js",
    "postbuild": "npm run copy",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "copy": "rimraf ../server/public && cpx -C \"public/*\" ../server/public && cpx -C \"public/images/*\" ../server/public/images && cpx -C \"dist/*.{js,ttf}\" ../server/public/js"
  },
  "dependencies": {
